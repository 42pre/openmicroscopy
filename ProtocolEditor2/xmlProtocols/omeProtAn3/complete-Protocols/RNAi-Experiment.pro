<?xml version="1.0" encoding="UTF-8"?><protocol input_type="Protocol Title" keywords="" elementName="RNAi Experiment" protocol_file_elementName="RNAi-Experiment.pro" substeps_collapsed=""><input input_type="Fixed Step" elementName="Fixed Step Example" substeps_collapsed="false"/><input description="Trypsinisation and seeding of HeLa cells" input_type="Protocol Title" keywords="" elementName="Plate HeLa" protocol_file_elementName="seedHela.pro" substeps_collapsed="true"><input input_type="Date" elementName="Date of seeding" substeps_collapsed="false" value=""/><input default="" input_type="Text Box" elementName="Comments" substeps_collapsed="false" value=""/><input description="Cells will typically be approaching confluency. Flask they are in gives some idea of number of cells. " dropdown_options="10 cm dish, 25 ml flask" input_type="Drop-down Menu" elementName="Flask" substeps_collapsed="false" value=""/><input input_type="Fixed Step" elementName="" substeps_collapsed="false"/><input description="Wash the HeLa cells 2 x in PBS pre-warmed to 37'C. " input_type="Fixed Step" elementName="Wash in PBS" substeps_collapsed="false"/><input description="Add a few ml of Trypsin solution. Aspirate off and add another. Take off most, leaving small volume. &#10;Wait for cells to round-up and detatch (under microscope if preferred)." input_type="Fixed Step" elementName="Trypsinise" substeps_collapsed="false"/><input default="DMEM" description="The growth media used for seeding cells. " input_type="Text" elementName="Media" substeps_collapsed="false" value=""/><input default="Penn/Strep&#10;Glutamate" description="Anything added to media" input_type="Text Box" elementName="Added to Media" substeps_collapsed="false" value=""/><input default="10" description="Resuspend in Media. " input_type="Number" elementName="Media Volume:" substeps_collapsed="false" units="ml" value=""/><input description="Flask or dish used to resuspend cells for experiment. &#10;(When simply propagating cells