<?xml version="1.0" encoding="utf-8"?>
<agent name="Browser">
  <resources>
	<iconFactories>
	  <!-- This entry is turned into an instance of: 
	       org.openmicroscopy.shoola.env.config.IconFactory
	       This object can then be used to retrieve any image file within
	       the directory pointed by the location tag. -->
	  <structuredEntry name="/resources/icons/Factory" type = "icons">
            <!-- The location tag specifies the FQN of the package that contains
		 the icon files. -->
            <location>org.openmicroscopy.shoola.agents.browser.graphx</location>
       </structuredEntry>
     </iconFactories>
	<fonts>
	  <!-- nothing in here yet, because I also haven't specified it. -->
	</fonts>
  </resources>
  <entry name="/agents/browser/config/useServerThumbs" type="boolean">false</entry>
  <entry name="/agents/browser/config/thumbnailWidth" type="integer">120</entry>
  <entry name="/agents/browser/config/thumbnailHeight" type="integer">120</entry>
  <entry name="/agents/browser/config/dummyDataset" type="integer">3</entry>
  <entry name="/agents/browser/config/semanticWidth" type="integer">150</entry>
  <entry name="/agents/browser/config/semanticHeight" type="integer">150</entry>

</agent>
