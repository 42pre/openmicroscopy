<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="org.ome.hibernate.model.ImageAnnotation" 
    table="image_annotations"
>

    <id
        name="attributeId"
        type="java.lang.Integer"
        column="attribute_id"
    >
        <generator class="assigned" />
    </id>

    <property
        name="theT"
        type="java.lang.Integer"
        column="the_t"
        length="4"
    />
    <property
        name="content"
        type="java.lang.String"
        column="content"
        length="-1"
    />
    <property
        name="theC"
        type="java.lang.Integer"
        column="the_c"
        length="4"
    />
    <property
        name="theZ"
        type="java.lang.Integer"
        column="the_z"
        length="4"
    />
    <property
        name="valid"
        type="java.lang.Boolean"
        column="valid"
        length="1"
    />
    <property
        name="timestamp"
        type="java.lang.Long"
        column="timestamp"
        length="8"
    />
    <property
        name="experimenter"
        type="java.lang.Integer"
        column="experimenter"
        length="4"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to Image -->
    <many-to-one
        name="image"
        class="org.ome.hibernate.model.Image"
        not-null="true"
    >
        <column name="image_id" />
    </many-to-one>

</class>
</hibernate-mapping>
