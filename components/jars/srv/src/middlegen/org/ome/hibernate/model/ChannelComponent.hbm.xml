<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="org.ome.hibernate.model.ChannelComponent" 
    table="channel_components"
>

    <id
        name="attributeId"
        type="java.lang.Integer"
        column="attribute_id"
    >
        <generator class="assigned" />
    </id>

    <property
        name="pixelsId"
        type="java.lang.Integer"
        column="pixels_id"
        length="4"
    />
    <property
        name="index"
        type="java.lang.Integer"
        column="index"
        length="4"
    />
    <property
        name="colorDomain"
        type="java.lang.String"
        column="color_domain"
        length="-1"
    />
    <property
        name="logicalChannel"
        type="java.lang.Integer"
        column="logical_channel"
        length="4"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to Image -->
    <many-to-one
        name="image"
        class="org.ome.hibernate.model.Image"
        not-null="true"
    >
        <column name="image_id" />
    </many-to-one>

</class>
</hibernate-mapping>
