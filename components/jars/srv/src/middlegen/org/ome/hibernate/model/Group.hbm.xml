<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="org.ome.hibernate.model.Group" 
    table="groups"
>

    <id
        name="attributeId"
        type="java.lang.Integer"
        column="attribute_id"
    >
        <generator class="assigned" />
    </id>

    <property
        name="name"
        type="java.lang.String"
        column="name"
        length="30"
    />
    <property
        name="moduleExecutionId"
        type="java.lang.Integer"
        column="module_execution_id"
        length="4"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to Dataset -->
    <set
        name="datasets"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="group_id" />
        </key>
        <one-to-many 
            class="org.ome.hibernate.model.Dataset"
        />
    </set>
    <!-- bi-directional one-to-many association to Image -->
    <set
        name="images"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="group_id" />
        </key>
        <one-to-many 
            class="org.ome.hibernate.model.Image"
        />
    </set>
    <!-- bi-directional one-to-many association to Project -->
    <set
        name="projects"
        lazy="true"
        inverse="true"
		cascade="all"
    >
        <key>
            <column name="group_id" />
        </key>
        <one-to-many 
            class="org.ome.hibernate.model.Project"
        />
    </set>
    <!-- bi-directional many-to-one association to Experimenter -->
    <many-to-one
        name="experimenterByLeader"
        class="org.ome.hibernate.model.Experimenter"
        not-null="true"
    >
        <column name="leader" />
    </many-to-one>
    <!-- bi-directional many-to-one association to Experimenter -->
    <many-to-one
        name="experimenterByContact"
        class="org.ome.hibernate.model.Experimenter"
        not-null="true"
    >
        <column name="contact" />
    </many-to-one>
    <!-- bi-directional many-to-many association to Experimenter -->
    <set
        name="experimenters"
        lazy="true"
		cascade="all"
        table="experimenter_group_map"
    >
        <key>
            <column name="group_id" />
        </key>
        <many-to-many
            class="org.ome.hibernate.model.Experimenter"
        >
            <column name="experimenter_id" />
        </many-to-many>
    </set>

</class>
</hibernate-mapping>
