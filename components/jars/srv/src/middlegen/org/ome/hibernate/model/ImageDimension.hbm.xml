<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="org.ome.hibernate.model.ImageDimension" 
    table="image_dimensions"
>

    <id
        name="attributeId"
        type="java.lang.Integer"
        column="attribute_id"
    >
        <generator class="assigned" />
    </id>

    <property
        name="pixelSizeC"
        type="java.lang.Float"
        column="pixel_size_c"
        length="4"
    />
    <property
        name="pixelSizeT"
        type="java.lang.Float"
        column="pixel_size_t"
        length="4"
    />
    <property
        name="pixelSizeX"
        type="java.lang.Float"
        column="pixel_size_x"
        length="4"
    />
    <property
        name="pixelSizeY"
        type="java.lang.Float"
        column="pixel_size_y"
        length="4"
    />
    <property
        name="pixelSizeZ"
        type="java.lang.Float"
        column="pixel_size_z"
        length="4"
    />

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to Image -->
    <many-to-one
        name="image"
        class="org.ome.hibernate.model.Image"
        not-null="true"
    >
        <column name="image_id" />
    </many-to-one>

</class>
</hibernate-mapping>
