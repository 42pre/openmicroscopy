<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="ome.model.ActualInput" 
    table="actual_inputs"
>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="actualInputId"
        type="java.lang.Integer"
        column="actual_input_id"
    >
        <generator class="sequence">
            <param name="sequence">this_seq_does_not_exist</param>
        </generator>
    </id>


    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to FormalInput -->
    <many-to-one
        name="formalInput"
        class="ome.model.FormalInput"
        not-null="true"
    >
        <column name="formal_input_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to ModuleExecution -->
    <many-to-one
        name="moduleExecutionByModuleExecutionId"
        class="ome.model.ModuleExecution"
        not-null="true"
    >
        <column name="module_execution_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to ModuleExecution -->
    <many-to-one
        name="moduleExecutionByInputModuleExecutionId"
        class="ome.model.ModuleExecution"
        not-null="true"
    >
        <column name="input_module_execution_id" />
    </many-to-one>

</class>
</hibernate-mapping>
