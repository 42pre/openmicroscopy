<!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >
  
<suite name="client.integration" >
  
  <test name="client.integration.active">
    <groups>
      <run>
        <exclude name="broken"/>
        <exclude name="ignore"/>
        <exclude name="manual"/>
      </run>        
    </groups>
    <packages>
      <package name="ome.client.*"/>
      <package name="ome.adapters.*"/>
    </packages>
  </test>

</suite>
