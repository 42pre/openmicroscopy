<!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >
  
<suite name="client.integration" >
  
  <test name="client.integration.broken">
    <groups>
      <run>
        <include name="broken"/>
        <include name="ignore"/>
      </run>        
    </groups>
    <packages>
      <package name="ome.*"/>
      <package name="omeis.*"/>
    </packages>
  </test>

</suite>
