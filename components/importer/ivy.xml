<ivy-module version="1.0">
  <info
      organisation="omero"
      module="importer"
      status="integration"/>
  <configurations>
    <conf name="build"/>
    <conf name="test"/>
    <conf name="client"/>
    <conf name="server"/>
  </configurations>
  <publications>
    <artifact name="importer" type="jar"/>
  </publications>
  <dependencies defaultconfmapping="build,test,client,server->default">
    <!-- Internal -->
    <dependency name="common" rev="${omero.version}" changing="true" conf="build->build;test->test;client->client;server->server"/>
    <dependency org="OME" name="bio-formats" rev="${versions.bio-formats}"/>
    <dependency org="OME" name="ome-java" rev="${versions.ome-java}"/>
    <!-- Loci -->
    <dependency org="loci" name="bufr" rev="1.1.00"/>
    <dependency org="loci" name="grib" rev="5.1.03"/>
    <dependency org="loci" name="ij" rev="unknown"/>
    <dependency org="loci" name="netcdf" rev="4.0"/>
    <dependency org="loci" name="poi" rev="${versions.poi}"/>
    <dependency org="loci" name="slf4j-jdk14" rev="1.0"/>
  </dependencies>
</ivy-module>
