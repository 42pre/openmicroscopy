<ivy-module version="1.0">
  <info
      organisation="omero"
      module="importer"
      status="integration"/>
  <configurations>
    <conf name="core"/>
    <conf name="build" extends="core"/>
    <conf name="test"  extends="core"/>
  </configurations>
  <publications>
    <artifact name="importer" type="jar" conf="core" />
  </publications>
  <dependencies>
    <!-- Internal -->
    <dependency name="common" rev="${omero.version}" changing="true" />
    <dependency org="OME" name="bio-formats" rev="${versions.bio-formats}" conf="build->default"/>
    <dependency org="OME" name="ome-java" rev="${versions.ome-java}" conf="build->default"/>
    <!-- Loci -->
    <dependency org="loci" name="bufr" rev="1.1.00" conf="build->default"/>
    <dependency org="loci" name="grib" rev="5.1.03" conf="build->default"/>
    <dependency org="loci" name="ij" rev="unknown" conf="build->default"/>
    <dependency org="loci" name="netcdf" rev="4.0" conf="build->default"/>
    <dependency org="loci" name="poi" rev="${versions.poi}" conf="build->default"/>
    <dependency org="loci" name="slf4j-jdk14" rev="1.0" conf="build->default"/>
  </dependencies>
</ivy-module>
