<ivy-module version="1.0">
  <info
      organisation="omero-tools"
      module="OmeroImporter"
      status="integration"/>
  <configurations>
    <conf name="build"/>
    <conf name="client"/>
  </configurations>
  <publications>
    <artifact name="OmeroImporter" type="jar"/>
  </publications>
  <dependencies defaultconfmapping="build,client->default">
    <!-- Internal -->
    <dependency org="omero" name="importer" rev="${omero.version}" changing="true" conf="build->build;client->client"/>
    <dependency org="omero" name="importer-common" rev="${omero.version}" changing="true" conf="build->build;client->client"/>
    <dependency org="omero-tools" name="omero_client" rev="${omero.version}" changing="true" conf="build->build;client->client"/>
    <!-- Gui -->
    <dependency org="gui" name="TableLayout" rev="unknown"/>
    <dependency org="gui" name="hsqldb" rev="1.8.0.9"/>
    <dependency org="gui" name="quaqua-filechooser-only" rev="unknown"/>
    <dependency org="gui" name="swingx" rev="2007_09_16"/>
    <dependency org="gui" name="libquaqua" rev="unknown"/>
    <dependency org="gui" name="omero-client-utils" rev="unknown"/>
    <dependency org="gui" name="ini4j" rev="0.3.2"/>
    <dependency org="gui" name="log4j" rev="1.2.14"/>
    <dependency org="gui" name="java-getopt" rev="1.0.13"/>
    <dependency org="gui" name="commons-httpclient" rev="${versions.commons-httpclient}"/>
    <dependency org="gui" name="commons-codec" rev="${versions.commons-codec}"/>
    <dependency org="OME" name="ome-java" rev="${versions.ome-java}"/>
    <!-- Loci -->
    <dependency org="OME" name="bio-formats" rev="${versions.bio-formats}"/>
    <dependency org="OME" name="loci-common" rev="${versions.bio-formats}"/>
    <dependency org="loci" name="poi-loci" rev="${versions.bio-formats}"/>
    <dependency org="loci" name="ome-xml" rev="${versions.bio-formats}"/>
    <dependency org="loci" name="bufr" rev="1.1.00"/>
    <dependency org="loci" name="grib" rev="5.1.03"/>
    <dependency org="loci" name="ij" rev="unknown"/>
    <dependency org="loci" name="netcdf" rev="4.0"/>
    <dependency org="loci" name="slf4j-log4j12" rev="1.4.3"/>

  </dependencies>
</ivy-module>
