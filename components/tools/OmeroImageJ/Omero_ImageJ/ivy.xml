    <conf name="build"/>
    <conf name="client"/>
  </configurations>
  <publications>
    <artifact name="Omero_ImageJ" type="jar"/>
  </publications>
  <dependencies defaultconfmapping="build,client->default">
    <!-- Internal -->
    <dependency org="omero" name="omero_client" rev="${omero.version}" changing="true" conf="build->build;client->client" transitive="false"/>
    <dependency org="OME" name="bio-formats" rev="${versions.bio-formats}"/>
    <dependency org="OME" name="loci-common" rev="${versions.bio-formats}"/>
    <dependency org="gui" name="forms" rev="${versions.forms}"/>
    <dependency org="ij" name="ij" rev="${versions.ij}"/>
  </dependencies>
</ivy-module>
