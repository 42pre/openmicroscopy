  <!-- 4.0 4/22 tdc: added keywords for meta keywords (which are required, and other meta tags are not). -->
  <!-- ================================================================ -->
  <!-- <keywords> element (for meta keywords)                                       -->
  <!-- ================================================================ -->
  <xsd:element name="keywords">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define keywords for this article.</title>
        <desc>Used to create META keywords.  Used by FAST and other search engines.  Use these guidelines to create keywords:  http://dw.raleigh.ibm.com/dwteam/search/search2003/guidelines/meta-guidelines.html</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="content" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The keywords, each separated by a comma.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <li> element                                                     -->
  <!-- ================================================================ -->
  <xsd:element name="li">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define an item in a list</title>
        <desc>Defines a list item for an ordered or unordered list.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="dl"/>
        <xsd:element ref="figure"/>
        <xsd:element ref="font"/>
        <xsd:element ref="i"/>
        <xsd:element ref="img"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="p"/>
        <xsd:element ref="pre"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:element ref="table"/>
        <xsd:element ref="ul"/>
        <xsd:group ref="specialCharacters"/>
        <!-- 4.0 4/26 tdc:  added font element per request #126 -->
      </xsd:choice>
      <xsd:attribute name="value" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>??? Is this the same as the HTML value attribute?  If so, this changes the item number for this item.  Subsequent list items are numbered accordingly as well. ???</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  
   
  <!-- 4.0 4/30 tdc:  removed meta element per design req. #206 -->
  <!-- 4.0 4/30 tdc:  added meta-last-updated element for last update meta tag per design req. #206 -->
  <!-- ================================================================ -->
  <!-- <meta-last-updated> element                                        -->
  <!-- ================================================================ -->
  <xsd:element name="meta-last-updated">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Meta last updated</title>
        <desc>Defines the HTML META tag that indicates when an editor last changed this file.  Not to be confused with date-updated, which contains the date when content was significantly updated and republished.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attributeGroup ref="dmy">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The day, month, and year (with leading zeros) an editor last changed this file.  Not to be confused with date-updated, which contains the date when content was significantly updated and republished.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attributeGroup>
      <xsd:attribute name="initials" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The initials (preferably at least three letters) of the editor who last changed this file.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <name> element    -->
  <!-- ================================================================ -->
  <xsd:element name="name">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Author name</title>
        <desc>Defines the name of an individual author.  Will remain until investigation and resolution of how to handle the different parts of author name (prefix, firstname, middlename, lastname, suffix)</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <ol> (ordered list) element                                      -->
  <!-- ================================================================ -->
  <xsd:element name="ol">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define an ordered list</title>
        <desc>Defines an ordered list.  It contains some number of &lt;li&gt; items.  You can also nest ordered and unordered lists inside the list.</desc>
        <!--, which can in turn contain other lists, such as ordered lists, unordered lists, or definition lists.  In other words, an unordered list can't be a child of an ordered list; you must put the unordered list (at the second level) inside a list item.</desc> -->
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice maxOccurs="unbounded">
        <xsd:element ref="li"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="ul"/>
      </xsd:choice>
      <xsd:attribute name="start" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the starting number for this list.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="type" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the type of numbering that should be used for this list.  Valid values are:</desc>
            <ul>
              <li>
                <code>1</code> - Items are numbered with Arabic numerals (<code>1</code>, <code>2</code>, <code>3</code>, <code>4</code>, <code>5</code>, and so forth). This is the default numbering style.</li>
              <li>
                <code>A</code> - Items are numbered with uppercase alpha characters (<code>A</code>, <code>B</code>, <code>C</code>, <code>...</code>, <code>AA</code>, <code>AB</code>, <code>AC</code>, and so forth)</li>
              <li>
                <code>a</code> - Items are numbered with lowercase alpha characters (<code>a</code>, <code>b</code>, <code>c</code>, <code>...</code>, <code>aa</code>, <code>ab</code>, <code>ac</code>, and so forth)</li>
              <li>
                <code>I</code> - Items are numbered with uppercase Roman numerals (<code>I</code>, <code>II</code>, <code>III</code>, <code>IV</code>, <code>V</code>, and so forth)</li>
              <li>
                <code>i</code> - Items are numbered with lowercase Roman numerals (<code>i</code>, <code>ii</code>, <code>iii</code>, <code>iv</code>, <code>v</code>, and so forth)</li>
            </ul>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <p> (paragraph) element                                          -->
  <!-- ================================================================ -->
  <xsd:element name="p">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a paragraph</title>
        <desc>Defines a paragraph of text.  </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="dl"/>
        <xsd:element ref="font"/>
        <xsd:element ref="i"/>
        <xsd:element ref="img"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="sidebar"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:element ref="table"/>
        <xsd:element ref="ul"/>
        <xsd:group ref="specialCharacters"/>
        <!-- <font> is evil, must be killed -->
      </xsd:choice>
      <xsd:attribute name="align" type="alignment">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the alignment for this paragraph.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <pdf> element                                                    -->
  <!-- ================================================================ -->
  <xsd:element name="pdf">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a link to a PDF file</title>
        <desc>Link to printer-friendly PDF version of the document.</desc>
        <note>Only required when we have a mechanism to automatically create a PDF. Right now, some DD content has a printer-friendly version, though it is not a PDF; objective is for PDF to be used as the printer-friendly version for all documents.  PDF must be A4 and letter.  Will continue to do printer friendly for domains using it.  If non-open standards printable doc, put in the Resources section.  Who has other non-open types and what are they?</note>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="paperSize" type="paperSize">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The paper size used in the PDF file. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="size" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The size in KB of the PDF file.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="url" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The URL of the PDF file.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <pre> element                                                    -->
  <!-- ================================================================ -->
  <xsd:element name="pre">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define preformatted content</title>
        <desc>Defines preformatted content.  All white space characters (spaces, newlines, tabs, and so forth) are preserved.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="i"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 4/20 tdc: removed productname and productfamily -->
  <!-- 4.0 4/19 tdc: renamed relatedlist to related-list -->
  <!-- ================================================================ -->
  <!-- <related-list> element                                            -->
  <!-- ================================================================ -->
  <xsd:element name="related-list">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a list of releated items</title>
        <desc>Content related to this document.  Other related content may be within dW, ibm.com, or include content outside ibm.com</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice maxOccurs="unbounded">
        <xsd:element ref="a"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 4/19 tdc: renamed to resource-list -->
  <!-- ================================================================ -->
  <!-- <resource-list> element                                           -->
  <!-- ================================================================ -->
  <xsd:element name="resource-list">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a list of resources</title>
        <desc>Defines a list of resources related to this article.  </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice maxOccurs="unbounded">
        <xsd:element ref="dl"/>
        <xsd:element ref="heading"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="p"/>
        <xsd:element ref="ul"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 4/19 tdc: renamed seriestitle to series-title -->
  <!-- ================================================================ -->
  <!-- <series-title> element                                            -->
  <!-- ================================================================ -->
  <xsd:element name="series-title">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define the series title for this article</title>
        <desc>Title of the series to which the document belongs.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <sidebar> element                                                -->
  <!-- ================================================================ -->
  <xsd:element name="sidebar">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a sidebar for this article</title>
        <desc>Sideline thought or concept that is not part of the general flow of the document.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="dl"/>
        <xsd:element ref="heading"/>
        <xsd:element ref="i"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="p"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:element ref="ul"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
      <xsd:attribute name="width">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The width in pixels that should be used for the sidebar.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="align" type="alignment">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The horizontal alignment of the sidebar. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <sub> (subscript) element                                        -->
  <!-- ================================================================ -->
  <xsd:element name="sub" type="xsd:string">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define subscript text</title>
        <desc>Defines text that should appear in subscript font.</desc>
      </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <subtitle> element                                               -->
  <!-- ================================================================ -->
  <xsd:element name="subtitle">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a subtitle for the article</title>
        <desc>Secondary title that may provide more information about the document, and/or contain catchy, creative wording in order to stimulate interest in the document.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <sup> (superscript) element                                      -->
  <!-- ================================================================ -->
  <xsd:element name="sup" type="xsd:string">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define superscript text</title>
        <desc>Defines text that should appear in superscript font.</desc>
      </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- 4.0 5/04 tdc:  Added for dWS editors to add support links to the hardcoded list provided by the stylesheet. -->
  <!-- ================================================================ -->
  <!-- <support-links> element                                               -->
  <!-- ================================================================ -->
  <xsd:element name="support-links">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Support links</title>
        <desc>One or more links to support information related to this piece of content.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice maxOccurs="unbounded">
        <xsd:element ref="a"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <table> element                                                  -->
  <!-- ================================================================ -->
  <xsd:element name="table">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a table</title>
        <desc>Defines a table.  In addition to formatting tabular information, you can use tables to have more control over the layout of information.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="thead" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="tr" maxOccurs="unbounded"/>
        <xsd:element ref="tfoot" minOccurs="0"/>
      </xsd:sequence>
      <xsd:attribute name="border" type="zeroOrOne">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Whether or not this table has a border.  </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="cellpadding" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The space in pixels that is added to all sides of the cells in the table.  A value of <code>cellpadding="3"</code> means none of the cells' contents will be closer than 3 pixels to the edge of the cells.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="cellspacing" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The space in pixels that is added between the cells in the table. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="cols">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The dimensions of the columns in the table.  For example, the markukp <code>cols="75 75 150 50"</code> creates four columns that are 75 pixels, 75 pixels, 150 pixels, and 50 pixels wide, respectively. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="width">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The width of the table.  You can specify this as a number of pixels (<code>width="350px"</code>) or as a percentage of the width of the page (<code>width="75%"</code>).</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 6/3 tdc:  Split target-content into target-content-file and target-content-page -->
  <!-- ================================================================ -->
  <!-- <target-content-file> element                                                    -->
  <!-- ================================================================ -->
  <xsd:element name="target-content-file">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Target content</title>
        <desc>The characteristics of, and links to, some target document, demo, presentation, code sample, etc. (as opposed to characteristics of *this* content).</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
    <!-- 4.0 6/18 tdc:  Several dWS-only attributes added for their downloads -->
     <xsd:attribute name="dws-crypto" type="cryptographic-method" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>dWS downloads only:  Identifies the method used to encrypt the target file.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="dws-esd-part-num" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>dWS downloads only:  Identifies the Lotus ESD part number of the target file.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="dws-esd-shortname" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>dWS downloads only:  Identifies the short name of the Lotus ESD target file.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="dws-form-name" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>dWS downloads only:  Identifies the name of the form used to initiate the download of the target file.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>    
     <xsd:attribute name="filename" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>The filename of the content.  Use the format xxxxx.yyy (include the file extension)</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <!-- 4.0 6/21 tdc:  Added for dWS; their file download forms require file path info -->
    <xsd:attribute name="file-path" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>Path information for this file.  Not displayed externally.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="file-type" type="file-type" use="required">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>File type of the target content.  Ex: war, jsp, pdf, zip </desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <!-- 4.0 6/9 tdc:  Made size optional b/c not all older files have it. -->
    <xsd:attribute name="size" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>The file size of the target content.  <code>78 KB</code> is an example value.  Included in alt tag for download gif. </desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="target-content-type" type="content-type" use="required">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>The type of the target content. Ex: technical article, demo, presentation, code sample, etc.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="file-description" type="file-description-50" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>A short description (50 characters or less) of the target content.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <!-- 4.0 6/4 tdc:  Added urls for ftp, http, dd -->
    <xsd:attribute name="link-method-ftp" type="yesno" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether the ftp transport protocol is available for the licensed download.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="url-ftp" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>The ftp URL of the target file.  Must include this, and/or url-http.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
      <xsd:attribute name="link-method-http" type="yesno" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether the http transport protocol is available for the licensed download.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="url-http" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>The http URL of the target file.  Must include this, and/or url-ftp.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
      <xsd:attribute name="link-method-dd" type="yesno" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether the Download Director method is available for the licensed download.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="url-download-director" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>The download-director URL of the target file.  Must include this, and/or url-http, and/or url-ftp.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>    
    <xsd:attribute name="show-license" type="yesno" use="required">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>Whether a license page should be displayed for the target file.  All code sample downloads require a license page.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="url-license" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>The URL of the license page, if there is one, that must be displayed for the target file.  All code sample downloads require a license page.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <!-- 4.0 6/7 tdc:  Is the xsl supposed to display some canned text if registration is required? -->
    <xsd:attribute name="registration-required" type="yesno" use="required">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>Whether the user must register in order to get the target file.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>  
  </xsd:element>
  
  <!-- 4.0 6/4 tdc:  Added target-content-page -->
  <!-- ================================================================ -->
  <!-- <target-content-page> element                                  -->
  <!-- ================================================================ -->
  <xsd:element name="target-content-page">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title></title>
        <desc>Used to define a link to a target page that contains a download, demo, presentation, code sample, etc.. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
    <!-- 4.0 6/6 tdc:  Added target-content-type to differentiate types, as done in target-content-file -->
    <xsd:attribute name="target-content-type" type="content-type" use="required">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>The type of the target content. Ex: white paper, article, demo, presentation, code sample, etc.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
      <xsd:attribute name="link-text" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The keywords, each separated by a comma.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="url-target-page" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The URL for the target page.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 4.0 6/7 tdc:  Is the xsl supposed to display some canned text if registration is required? -->
      <xsd:attribute name="registration-required" type="yesno" use="required">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>Whether the user must register to get the target file.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    </xsd:complexType>
  </xsd:element>

  <!-- 4.0 6/7 tdc:  Removed target-content-info, no longer need given target-content-file and target-content-page -->
  
  <!-- ================================================================ -->
  <!-- <td> element                                                     -->
  <!-- ================================================================ -->
  <xsd:element name="td">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a table cell</title>
        <desc>Defines the contents of a table cell.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="dl"/>
        <xsd:element ref="font"/>
        <xsd:element ref="i"/>
        <xsd:element ref="img"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="p"/>
        <xsd:element ref="pre"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:element ref="table"/>
        <xsd:element ref="ul"/>
        <xsd:group ref="specialCharacters"/>
        <!-- <font> is evil, must be killed -->
        <!-- 4/7/03: Added by Doug, bad style... -->
      </xsd:choice>
      <xsd:attribute name="align" type="alignment">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The alignment of this cell.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="bgcolor" type="xslfoColor">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The background color of this table cell.  This can be specified in two ways: as an RGB value (<code>bgcolor="#99CC33"</code>) or as one of the sixteen color values defined in the XSL-FO standard:
				<table cols="25% 25% 25% 25%">
                <tr>
                  <td>aqua</td>
                  <td>black</td>
                  <td>blue</td>
                  <td>fuchsia</td>
                </tr>
                <tr>
                  <td>gray</td>
                  <td>green</td>
                  <td>lime</td>
                  <td>maroon</td>
                </tr>
                <tr>
                  <td>navy</td>
                  <td>olive</td>
                  <td>purple</td>
                  <td>red</td>
                </tr>
                <tr>
                  <td>silver</td>
                  <td>teal</td>
                  <td>white</td>
                  <td>yellow</td>
                </tr>
              </table>
            </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="colspan" type="xsd:positiveInteger">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the number of columns this cell should span. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="rowspan" type="xsd:positiveInteger">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the number of rows this cell should span.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="width">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the width of this cell.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="valign">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the vertical alignment of this cell.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <tfoot> element                                                  -->
  <!-- ================================================================ -->
  <xsd:element name="tfoot">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a table footer</title>
        <desc>Defines a footer row for a table. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice maxOccurs="unbounded">
        <xsd:element ref="td"/>
      </xsd:choice>
      <xsd:attribute name="align" type="alignment">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the horizontal alignment of this cell.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="bgcolor" type="xslfoColor">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The background color of this table cell.  This can be specified in two ways: as an RGB value (<code>bgcolor="#99CC33"</code>) or as one of the sixteen color values defined in the XSL-FO standard:
				<table cols="25% 25% 25% 25%">
                <tr>
                  <td>aqua</td>
                  <td>black</td>
                  <td>blue</td>
                  <td>fuchsia</td>
                </tr>
                <tr>
                  <td>gray</td>
                  <td>green</td>
                  <td>lime</td>
                  <td>maroon</td>
                </tr>
                <tr>
                  <td>navy</td>
                  <td>olive</td>
                  <td>purple</td>
                  <td>red</td>
                </tr>
                <tr>
                  <td>silver</td>
                  <td>teal</td>
                  <td>white</td>
                  <td>yellow</td>
                </tr>
              </table>
            </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="valign" type="vertical">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the vertical alignment of the image.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <thead> element                                                  -->
  <!-- ================================================================ -->
  <xsd:element name="thead">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a table header</title>
        <desc>Defines a header row for a table.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice maxOccurs="unbounded">
        <xsd:element ref="td"/>
      </xsd:choice>
      <xsd:attribute name="align" type="alignment">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the horizontal alignment of this cell.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="bgcolor" type="xslfoColor">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The background color of this table cell.  This can be specified in two ways: as an RGB value (<code>bgcolor="#99CC33"</code>) or as one of the sixteen color values defined in the XSL-FO standard:
				<table cols="25% 25% 25% 25%">
                <tr>
                  <td>aqua</td>
                  <td>black</td>
                  <td>blue</td>
                  <td>fuchsia</td>
                </tr>
                <tr>
                  <td>gray</td>
                  <td>green</td>
                  <td>lime</td>
                  <td>maroon</td>
                </tr>
                <tr>
                  <td>navy</td>
                  <td>olive</td>
                  <td>purple</td>
                  <td>red</td>
                </tr>
                <tr>
                  <td>silver</td>
                  <td>teal</td>
                  <td>white</td>
                  <td>yellow</td>
                </tr>
              </table>
            </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="valign" type="vertical">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the vertical alignment of the image.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
