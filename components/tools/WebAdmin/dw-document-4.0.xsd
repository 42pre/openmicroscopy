  <!-- ================================================================ -->
  <!-- Attribute Groups                                                 -->
  <!-- ================================================================ -->
  <!-- Day/Month/Year attributes                                        -->
  <!-- ================================================================ -->
  <!-- 4.0 06/10 tdc:  Had switched to gDay, gMonth, gYear.  But now adding leading zeros for day, month.  No longer using recentyear or updatedyear - don't need. -->
  <xsd:attributeGroup name="dmy">
    <xsd:attribute name="day" type="day">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>The day of the month the article was created or updated.  Leading zeros are required.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="month" type="month" use="required">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>The month of the year the article was created or updated.  Leading zeros are required.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="year" type="year" use="required">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>The year the article was created or updated. </desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:attributeGroup>
  <!-- ================================================================ -->
  <!-- Data types                                                       -->
  <!-- ================================================================ -->
  <!-- alignment - left, right, or center.                              -->
  <!-- ================================================================ -->
  <xsd:simpleType name="alignment">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="center">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Centers the item horizontally.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="left">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Left-aligns the item horizontally.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="right">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Right-aligns the item horizontally.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ================================================================ -->
  <!-- auto - auto, yes, or no.                                         -->
  <!-- ================================================================ -->
  <xsd:simpleType name="auto">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="auto">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Determines the value automatically.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="yes">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The value is set to <code>yes</code>.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="no">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The value is set to <code>no</code>.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- 4.0 5/04 tdc:  new cd-range for dWS -->
  <!-- ================================================================ -->
  <!-- CD number (or range)                                       -->
  <!-- ================================================================ -->
  <xsd:attributeGroup name="cd-range">
    <xsd:attribute name="cd" type="yesno" default="no">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>Defines whether or not this  bundle is available on CD-ROM</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="range-bottom" type="xsd:positiveInteger" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>Specify the number of the CD that this bundle or component appears on.  If the bundle or component code is spread across several CDs, specify the number of the first CD that this bundle or component appears on.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="range-top" type="xsd:positiveInteger" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">
          <desc>If the bundle or component code is spread across several CDs, specify the number of the last CD that this bundle or component appears on.  This value must be greater than the value for range-bottom.</desc>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:attributeGroup>
