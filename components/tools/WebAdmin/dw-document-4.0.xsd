<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="E:\developerworks\library\schema\3.0\ia\schema-to-article.xsl"?> <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:dw="http://www.ibm.com/developerWorks/" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="4.0">
  <!-- ================================================================ -->
  <!-- Annotations that define other content for the automatically-     -->
  <!-- generated article about our xml tag set.                                              -->
  <!-- ================================================================ -->
  <xsd:annotation id="setup">
    <xsd:documentation xml:lang="en">
      <mainTitle>The developerWorks document schema</mainTitle>
      <referenceSectionTitle>Tag Reference</referenceSectionTitle>
      <filename>article</filename>
      <img>images/tutorial.jpg</img>
      <alt>The developerWorks document schema</alt>
      <feedback-link>feedback-panel</feedback-link>
      <content-area-primary>XML</content-area-primary>
      <abstract>This article documents the production-level 4.0 schema for the documents we create using xml, including articles, side files, summary pages, dWS landing pages, dWS, component pages, and dWS standalone pages.</abstract>
      <exit_url>http://www.ibm.com/developerWorks/xml/</exit_url>
    </xsd:documentation>
  </xsd:annotation>
  <xsd:annotation id="before">
    <xsd:documentation source="dw-document-before.xml" xml:lang="en"/>
    <xsd:documentation source="editorial-guidelines.xml" xml:lang="en"/>
  </xsd:annotation>
  <xsd:annotation id="after">
    <xsd:documentation source="submitting-your-article.xml" xml:lang="en"/>
    <xsd:documentation source="dw-document-after.xml" xml:lang="en"/>
  </xsd:annotation>
  <!-- ================================================================ -->
  <!-- Elements that define document types                              -->
  <!-- ================================================================ -->
  <!-- <dw-document> element, the root element for everything           -->
  <!-- ================================================================ -->
  <xsd:element name="dw-document">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>The root element of all dW content</title>
        <desc>Contains a dW document.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice>
        <xsd:element ref="dw-article"/>
        <xsd:element ref="dw-sidefile"/>
        <xsd:element ref="dw-subscription-component"/>
        <xsd:element ref="dw-subscription-landing"/>
        <xsd:element ref="dw-subscription-standalone"/>
        <xsd:element ref="dw-summary"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <dw-article> element                                   -->
  <!-- ================================================================ -->
  <xsd:element name="dw-article">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a developerWorks article</title>
        <desc>Contains a developerWorks article.  This is the root element of most documents on the dW site. </desc>
        </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="id" minOccurs="0"/>
        <xsd:element ref="keywords"/>
        <xsd:element ref="meta-last-updated" minOccurs="0"/>
        <xsd:element ref="content-area-primary"/>
        <xsd:element ref="content-area-secondary" minOccurs="0" maxOccurs="unbounded"/>
        <!-- 4.0 05/14 tdc: removed breadcrumbappend -->
        <xsd:element ref="series-title" minOccurs="0"/>
        <xsd:element ref="title"/>
        <xsd:element ref="subtitle" minOccurs="0"/>
        <xsd:element ref="forum-url" minOccurs="0"/>
        <!-- 4.0 6/4 tdc:  Split target-content into target-content-file and target-content-page -->
        <xsd:element ref="target-content-file" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="target-content-page" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="pdf" minOccurs="0"/>
        <xsd:element ref="author" maxOccurs="unbounded"/>
        <xsd:element ref="date-published"/>
        <xsd:element ref="date-updated" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="abstract"/>
        <xsd:element ref="abstract-extended" minOccurs="0"/>
        <xsd:element ref="column-info" minOccurs="0"/>
        <xsd:element ref="docbody"/>
        <!-- 4.0 7/21 tdc:  Changed maxOccurs from unbounded to 1 -->
        <xsd:element ref="attribution" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="trademarks" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="related-list" minOccurs="0"/>
        <xsd:element ref="resource-list" minOccurs="0"/>
        <!-- 4.0 5/07 tdc:  use target-content instead of download-feature -->
        <!-- 4.0 4/30 tdc:  added keywords -->
        <!-- 4.0 4/30 tdc:  removed meta element per design req. #206 -->
        <!-- 4.0 4/30 tdc:  added meta-last-updated element for last update meta tag per design req. #206 -->
        <!-- 4.0 4/20 tdc: added keywords for META keywords, required -->
        <!-- 4.0 4/20 tdc: removed productname and productfamily -->
        <!-- 4.0 4/15 tdc: replaced content-area with content-area-primary and content-area-secondary -->
        <!-- 4.0 4/19 tdc: renamed seriestitle to series-title -->
        <!-- 4.0 4/19 tdc: renamed forumurl to forum-url -->
        <!-- 4.0 4/19 tdc: renamed date to date-published -->
        <!-- 4.0 4/23 tdc:  Do we still need attribution, given we have trademark? I think so.  -->
        <!-- 4.0 4/19 tdc: renamed relatedlist to related-list -->
        <!-- 4.0 4/19 tdc: renamed resourcelist to resource-list -->
      </xsd:sequence>
      <xsd:attribute name="skill-level" type="skill-level" use="optional"/>
      <xsd:attribute name="local-site" type="site" use="required"/>
      <xsd:attribute name="ratings-form" type="auto" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Whether this article should have a ratings form.  The default value is <code>auto</code>, which means the formatter decides whether the article should have a ratings form or not. </desc>
            <note>Individual team implementations for phase 1. Consistent across the supersite for phase 2.  For phase 1, the rating will be for the primary content-area and the stylesheet determines based on what's entered in the xml.</note>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="related-contents" type="auto" use="required"/>
      <xsd:attribute name="toc" type="auto" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Whether this article should have a table of contents.  The default value is <code>auto</code>, which means the formatter decides if the article has enough headings to make a table of contents useful. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 4.0 9/28 tdc:  Removed display-products-text attribute per Sera -->
      <!-- 4.0 6/11 tdc:  Added trigger to display optional paragraph after abstract to get products avail. via dWS -->
      <!-- 4.0 4/15 tdc: changed level to skill-level -->
      <!-- 4.0 4/15 tdc: changed  localsite to local-site -->
      <!-- 4.0 4/15 tdc: changed ratings to ratings-form -->
      <!-- 4.0 4/15 tdc: changed related_contents to related-contents -->
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <dw-sidefile> element                                            -->
  <!-- ================================================================ -->
  <xsd:element name="dw-sidefile">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a sidefile associated with a dW article</title>
        <desc>Contains a sidefile for a developerWorks article.  This is typically rendered as a sidefile within an article. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="id" minOccurs="0"/>
        <xsd:element ref="keywords"/>
        <xsd:element ref="meta-last-updated" minOccurs="0"/>
        <xsd:element ref="content-area-primary"/>
        <xsd:element ref="content-area-secondary" minOccurs="0" maxOccurs="unbounded"/>
        <!-- 4.0 05/14 tdc: removed breadcrumbappend -->
        <xsd:element ref="series-title" minOccurs="0"/>
        <xsd:element ref="title"/>
        <xsd:element ref="subtitle" minOccurs="0"/>
        <xsd:element ref="date-published"/>
        <xsd:element ref="date-updated" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="forum-url" minOccurs="0"/>
        <xsd:element ref="docbody"/>
        <!-- 4.0 7/21 tdc:  Changed maxOccurs from unbounded to 1 -->
        <xsd:element ref="attribution" minOccurs="0" maxOccurs="1"/>
        <!-- 4.0 4/30 tdc:  removed meta element per design req. #206 -->
        <!-- 4.0 4/30 tdc:  added meta-last-updated element for last update meta tag per design req. #206 -->
        <!-- 4.0 4/20 tdc: added keywords for META keywords, required -->
        <!-- 4.0 4/15 tdc: replaced content-area with content-area-primary and content-area-secondary -->
        <!-- 4.0 4/20 tdc: removed productname and productfamily -->
        <!-- 4.0 4/19 tdc: renamed seriestitle to series-title -->
        <!-- 4.0 4/19 tdc: renamed date to date-published -->
        <!-- 4.0 4/19 tdc: renamed forumurl to forum-url -->
      </xsd:sequence>
      <xsd:attribute name="local-site" type="site" use="required"/>
      <!-- 4.0 4/15 tdc: changed  localsite to local-site -->
    </xsd:complexType>
  </xsd:element>
  
  <!-- 4.0 6/3 tdc:  Added for dws component pages -->
  <!-- ================================================================ -->
  <!-- <dw-subscription-component> element                                   -->
  <!-- ================================================================ -->
  <xsd:element name="dw-subscription-component">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a developerWorks Subscription (formerly Toolbox) component page</title>
        <desc>Contains a developerWorks Subscription component page.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="id" minOccurs="0"/>
        <xsd:element ref="keywords"/>
        <xsd:element ref="meta-last-updated" minOccurs="0"/>
        <xsd:element ref="content-area-primary"/>
        <xsd:element ref="content-area-secondary" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="title"/>
        <xsd:element ref="date-integration"/>
        <xsd:element ref="abstract"/>
        <xsd:element ref="dws-bundle-title"/>
        <xsd:element ref="dws-overview"/>
        <xsd:element ref="dws-more-about" minOccurs="0"/>
        <xsd:element ref="dws-platforms"/>
        <xsd:element ref="dws-requirements" minOccurs="0"/>
        <xsd:element ref="dws-languages" minOccurs="0"/>
        <xsd:element ref="bundle-components"/>
        <xsd:element ref="dws-download"/>
        <xsd:element ref="dws-install-instructions"/>
        <xsd:element ref="dws-availability"/>                
        <xsd:element ref="support-links" minOccurs="0" maxOccurs="unbounded"/> 
        <xsd:element ref="related-list" minOccurs="0"/>
      </xsd:sequence>
      <xsd:attribute name="subscription-level" type="subscription-level" use="required"/>
      <xsd:attribute name="related-contents" type="yesno" use="required"/>
      <xsd:attribute name="toc" type="yesno" use="required"/>     
    </xsd:complexType>
  </xsd:element>
  
  <!-- 4.0 6/7 tdc:  Reordered elements in the order they appear on the page, center column first -->
  <!-- ================================================================ -->
  <!-- <dw-subscription-landing> element                                   -->
  <!-- ================================================================ -->
  <xsd:element name="dw-subscription-landing">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a developerWorks Subscription (formerly Toolbox) landing page</title>
        <desc>Contains a developerWorks Subscription landing page.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="id" minOccurs="0"/>
        <xsd:element ref="keywords"/>
        <xsd:element ref="meta-last-updated" minOccurs="0"/>
        <xsd:element ref="content-area-primary"/>
        <xsd:element ref="content-area-secondary" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="title"/>
        <xsd:element ref="date-integration"/>
        <xsd:element ref="abstract"/>
        <xsd:element ref="dws-overview"/>
        <xsd:element ref="dws-more-about" minOccurs="0"/>
        <xsd:element ref="dws-platforms"/>
        <xsd:element ref="dws-requirements" minOccurs="0"/>
        <xsd:element ref="dws-languages" minOccurs="0"/>
        <xsd:element ref="bundle-components"/>
        <xsd:element ref="dws-availability"/>   
        <xsd:element ref="support-links" minOccurs="0" maxOccurs="unbounded"/>        
        <xsd:element ref="related-list" minOccurs="0"/>
        <!-- 4.0 5/10 tdc: removed breadcrumbappend -->
        <!-- 4.0 5/10 tdc: removed series-title -->
        <!-- 4.0 5/10 tdc: removed subtitle -->
        <!-- 4.0 5/10 tdc: replaced date-published with date-integration (required) -->
        <!-- 4.0 5/10 tdc: removed date-updated -->
        <!-- 4.0 4/15 tdc: renamed bundleavailability to dws-availability -->
        <!-- 4.0 4/15 tdc: renamed bundleoverview to dws-overview -->
        <!-- 4.0 4/15 tdc: renamed bundleplatforms to dws-platforms -->
        <!-- 4.0 4/15 tdc: renamed bundlelanguages to dws-languages -->
        <!-- 4.0 4/15 tdc: renamed bundlemoreabout to dws-more-about -->
        <!-- 4.0 4/15 tdc: renamed bundlerequirements to dws-requirements -->
        <!-- 4.0 4/15 tdc: added support-links -->
        <!-- 4.0 5/04 tdc:  made dws-availability required -->
        <!-- 4.0 5/04 tdc:  Added more-info (prev. "web site" info in body) -->
        <!-- 4.0 4/30 tdc:  removed meta element per design req. #206 -->
        <!-- 4.0 4/30 tdc:  added meta-last-updated element for last update meta tag per design req. #206 -->
        <!-- 4.0 4/20 tdc: added keywords for META keywords, required -->
        <!-- 4.0 4/15 tdc: replaced content-area with content-area-primary and content-area-secondary -->
        <!-- 4.0 4/19 tdc: renamed relatedlist to related-list -->
      </xsd:sequence>
      <xsd:attribute name="subscription-level" type="subscription-level" use="required"/>
      <xsd:attribute name="related-contents" type="yesno" use="required"/>
      <xsd:attribute name="toc" type="yesno" use="required"/>        
      <!-- 4.0 4/15 tdc: changed subscriptionlevel to subscription-level -->
      <!-- 4.0 4/15 tdc: changed related_contents to related-contents -->
    </xsd:complexType>
  </xsd:element>
  
  <!-- ================================================================ -->
  <!-- <dw-subscription-standalone> element                                   -->
  <!-- ================================================================ -->
  <xsd:element name="dw-subscription-standalone">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a developerWorks Subscription (formerly Toolbox) standalone page</title>
        <desc>Contains a developerWorks Subscription standalone page.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="id" minOccurs="0"/>
        <xsd:element ref="keywords"/>
        <xsd:element ref="meta-last-updated" minOccurs="0"/>
        <xsd:element ref="content-area-primary"/>
        <xsd:element ref="content-area-secondary" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="title"/>
        <xsd:element ref="date-integration"/>
        <xsd:element ref="abstract"/>
        <xsd:element ref="dws-overview"/>
        <xsd:element ref="dws-more-about" minOccurs="0"/>
        <xsd:element ref="dws-platforms"/>
        <xsd:element ref="dws-requirements" minOccurs="0"/>
        <xsd:element ref="dws-languages" minOccurs="0"/>
        <xsd:element ref="dws-download"/>
        <xsd:element ref="dws-install-instructions"/>
        <xsd:element ref="dws-availability"/>                
        <xsd:element ref="support-links" minOccurs="0" maxOccurs="unbounded"/> 
        <xsd:element ref="related-list" minOccurs="0"/>
      </xsd:sequence>
      <xsd:attribute name="subscription-level" type="subscription-level" use="required"/>
      <xsd:attribute name="related-contents" type="yesno" use="required"/>
      <xsd:attribute name="toc" type="yesno" use="required"/>     
    </xsd:complexType>
  </xsd:element>
  
  <!-- 4.0 4/27 tdc: added dw-summary -->
  <!-- ================================================================ -->
  <!-- <dw-summary> element                                   -->
  <!-- ================================================================ -->
  <xsd:element name="dw-summary">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a developerWorks summary page</title>
        <desc>Contains a developerWorks summary page for technical documents, demos, presentations, code samples, product information, etc..</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="id" minOccurs="0"/>
        <xsd:element ref="keywords"/>
        <xsd:element ref="meta-last-updated" minOccurs="0"/>
        <xsd:element ref="content-area-primary"/>
        <xsd:element ref="content-area-secondary" minOccurs="0" maxOccurs="unbounded"/>
        <!-- 4.0 05/14 tdc: removed breadcrumbappend -->
        <xsd:element ref="series-title" minOccurs="0"/>
        <xsd:element ref="title"/>
        <xsd:element ref="subtitle" minOccurs="0"/>
        <xsd:element ref="forum-url" minOccurs="0"/>
        <xsd:element ref="author" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="date-published"/>
        <xsd:element ref="date-updated" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="abstract"/>
        <xsd:element ref="abstract-extended" minOccurs="0"/>
        <xsd:element ref="docbody"/>
        <!-- 4.0 6/4 tdc:  Split target-content into target-content-file and target-content-page -->
        <xsd:element ref="target-content-file" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="target-content-page" minOccurs="0" maxOccurs="unbounded"/>
        <!-- 4.0 7/21 tdc:  Changed maxOccurs from unbounded to 1 -->
        <xsd:element ref="attribution" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="trademarks" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="resource-list" minOccurs="0"/>
        <!-- 4.0 5/7 tdc:  removed related-list per discussion with EGD -->
        <!-- 4.0 4/30 tdc:  removed meta element per design req. #206 -->
        <!-- 4.0 4/30 tdc:  added meta-last-updated element for last update meta tag per design req. #206 -->
      </xsd:sequence>
      <xsd:attribute name="skill-level" type="skill-level" use="optional"/>
      <xsd:attribute name="local-site" type="site" use="required"/>
      <xsd:attribute name="ratings-form" type="auto" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Whether this document should have a ratings form.  The default value is <code>auto</code>, which means the formatter decides whether the article should have a ratings form or not. </desc>
            <note>Individual team implementations for phase 1. Consistent across the supersite for phase 2.  For phase 1, the rating will be for the primary content area and the stylesheet determines based on what's entered in the xml.</note>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="related-contents" type="auto" use="required"/>
      <xsd:attribute name="toc" type="auto" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Whether this article should have a table of contents.  The default value is <code>auto</code>, which means the formatter decides if the article has enough headings to make a table of contents useful. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 4.0 4/15 tdc: changed level to skill-level -->
      <!-- 4.0 4/15 tdc: changed localsite to local-site -->
      <!-- 4.0 4/15 tdc: changed ratings to ratings-form -->
      <!-- 4.0 4/15 tdc: changed related_contents to related-contents -->
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- Elements for content                                             -->
  <!-- ================================================================ -->
  <!-- <a> (anchor) element   -->
  <!-- ================================================================ -->
  <xsd:element name="a">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a hypertext anchor</title>
        <desc>Defines a hypertext link.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="img"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
      <xsd:attribute name="href" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the target URL of this hypertext link. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="name" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Gives this location a name.  Named anchors can be referenced just like external URLs.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="onClick" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the action that should take place when the user clicks this link.  This is typically JavaScript code or a call to a JavaScript function. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="target" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines a target for this link.  You can use this attribute to refer to a named browser window.  A value of <code>_blank</code> means the linked document will appear in a new browser window.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 4.0 6/1 tdc:  Removed the trackclick attribute per editors -->
    </xsd:complexType>
    <!-- Defined as ANY in the DTD -->
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <abstract> element                                               -->
  <!-- ================================================================ -->
  <xsd:element name="abstract" type="xsd:string">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>abstract</title>
        <desc>Brief overview of the document that will be displayed if no abstract-extended element is present, as well as in content views and search results; should contain information to help the user decide whether to read the document.  Used to create the meta abstract tag, which contributes to relevance in the ibm.com FAST search. Also used for e-mail to a friend.  No HTML coding allowed since this is used for the meta tags.  Use these guidelines to create: http://dw.raleigh.ibm.com/dwteam/search/search2003/guidelines/meta-guidelines.doc</desc>
      </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <abstract-extended> element                                               -->
  <!-- ================================================================ -->
  <xsd:element name="abstract-extended">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>A long summary of the article</title>
        <desc>Optional, longer abstract that can be used as a page element of the document.  Use if you want a longer abstract or need to use HTML coding in the abstract that is a page element in the document.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="p"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <attribution> element                                            -->
  <!-- ================================================================ -->
  <xsd:element name="attribution">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define the external source of an article</title>
        <desc>Credit line if source of the article is external, like an external magazine (JavaWorld), or Web site.</desc>
        <note>Use this value to create the DC.Publisher meta tag according to ibm.com meta tag standards.  If this value is not specified with a third party name, then fill in "IBM Corporation"</note>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="p"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
      <xsd:attribute name="type" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The source of the article.  </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 12/1/03 TDC:  Removed default='javaworld'.  Should probably use an enumeration here in 2.2+ to trigger different attribution statements in xsl. -->
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <author> element        -->
  <!-- ================================================================ -->
  <xsd:element name="author">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define information about the author of the article</title>
        <desc>Can have multiple authors.  Future (post-3.0):  Authors can have multiple bios.  Currently "name" is used for author name in schema until investigation is complete on how to handle parts of name (prefix, firstname, middlename, lastname, suffix).</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="bio"/>
        <xsd:element ref="img" minOccurs="0"/>
        <xsd:element ref="name"/>
      </xsd:sequence>
      <xsd:attribute name="company" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The company the author works for.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="email" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The author's email address.  Need some method so that author's e-mail address isn't visible on the page.  Once this requirement is met, should be Required.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="jobtitle" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The author's job title.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 4.0 7/23 tdc:  Rename email_cc to email-cc -->
      <xsd:attribute name="email-cc" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Allows someone on the dW team, like an acquisition editor, to be copied on author e-mails.  Need some method so that cc e-mail address isn't visible on the page.  Once this requirement is met, should be Required.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <b> (bold) element                                               -->
  <!-- ================================================================ -->
  <xsd:element name="b">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define boldfaced text</title>
        <desc>Defines boldfaced text. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="font"/>
        <xsd:element ref="i"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
        <!-- 4/7/03 Doug added <font> temporarily -->
      </xsd:choice>
      <xsd:attribute name="class" type="colorname">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines the color scheme used by this element's text.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <bio> element                                                    -->
  <!-- ================================================================ -->
  <xsd:element name="bio">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>A short biography of the author</title>
        <desc>Defines a short biography of the author.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="p"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <blockquote> element                                             -->
  <!-- ================================================================ -->
  <xsd:element name="blockquote">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a lengthy quote</title>
        <desc>Defines a long quote.  The text is indented on the left and right margins.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <br> (break) element                                             -->
  <!-- ================================================================ -->
  <xsd:element name="br">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a line break</title>
        <desc>Defines a line break in the text. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:simpleType>
      <xsd:restriction base="empty"/>
    </xsd:simpleType>
  </xsd:element>
  <!-- 4.0 5/20 tdc:  removed breadcrumbappend element definition -->
  <!-- 4.0 5/04 tdc:  renamed from bundlecomponent to bundle-component -->
  <!-- ================================================================ -->
  <!-- <bundle-component> element                                       -->
  <!-- ================================================================ -->
  <xsd:element name="bundle-component">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>A link to a developerWorks Subscription bundle component page.</title>
        <desc>A link to a developerWorks Subscription bundle component page.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="text" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The text of the link. </desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 4.0 6/18 tdc:  The url is req'd for dw-subscription-landing pages.  It is optional for components on component or
      standalone pages because the subject component of a particular page should not be a link. -->
      <xsd:attribute name="url" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The URL of the link.  The url is req'd for components on dw-subscription-landing pages.  It is optional for components on component or standalone pages because the subject component of a particular page should not be a link.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 11/11 tdc:  removed installorder and part attrib's since not needed by tbox (info is already contained in original component title) -->
      <!-- <xsd:attribute name="installorder" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The order within the install process that this component should be installed.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="part" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>This component is one part out of entire number of parts that make up the bundle.  The value for part is the "X" in the phrase, "Part X of Y".</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute> -->
      <!-- ** Question: Should we have text be optional and use the URL if text isn't there? -->
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 5/04 tdc:  renamed from bundlecomponents to bundle-components -->
  <!-- ================================================================ -->
  <!-- <bundle-components> element                                       -->
  <!-- ================================================================ -->
  <xsd:element name="bundle-components">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Section containing links to multiple developerWorks Subscription bundle component pages.</title>
        <desc>Section containing links to multiple developerWorks Subscription bundle component pages.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="heading"/>
        <xsd:element ref="bundle-component" maxOccurs="unbounded"/>
      </xsd:sequence>
      <!-- 11/11 tdc:  removed totalparts attrib since not needed by tbox (info is already contained in original component title) -->
      <!-- <xsd:attribute name="totalparts" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The total number of parts (components) that make up the bundle.  The value for totalparts is the "Y" in the phrase, "Part X of Y".</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute> -->
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <code> element                                                   -->
  <!-- ================================================================ -->
  <xsd:element name="code">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a code listing</title>
        <desc>The stylesheet allows code to be inline or section.  The contents of this element are displayed in a monospaced font, with all whitespace preserved from the original XML source.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="font"/>
        <xsd:element ref="heading"/>
        <xsd:element ref="i"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
      <xsd:attribute name="type" type="inline" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The type of code listing.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="width">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The width in characters of this code listing.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <column-info> element                                            -->
  <!-- ================================================================ -->
  <xsd:element name="column-info">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define meta information for this column</title>
        <desc>Defines meta information for this column, such as an icon or a title.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="col-icon" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines an icon for this column.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="col-name" type="xsd:string" use="required">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines a name for this column.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- ** This is defined as ANY in the DTD, but it looks like it's empty. -->
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 4/15 tdc: replaced content-area with content-area-primary and content-area-secondary. -->
  <!-- ================================================================ -->
  <!-- <content-area-primary> element                                                   -->
  <!-- ================================================================ -->
  <xsd:element name="content-area-primary">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define the primary content area for this article</title>
        <desc>Indicates the primary dW product and/or technology content area that should contain this document.   Used to create page elements like breadcrumb trail. Search:  Request meta tag. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="name" type="content-area" use="required"/>
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 4/15 tdc: replaced content-area with content-area-primary and content-area-secondary. -->
  <!-- ================================================================ -->
  <!-- <content-area-secondary> element                                                   -->
  <!-- ================================================================ -->
  <xsd:element name="content-area-secondary">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define the secondary content area for this article</title>
        <desc>Indicates a secondary dW product and/or technology content area that should contain this document.  Request meta tag. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="name" type="content-area" use="required"/>
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 5/10 tdc:  Added date-integration for dWS-->
  <!-- ================================================================ -->
  <!-- <date-integration> element                                                   -->
  <!-- ================================================================ -->
  <xsd:element name="date-integration">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>The dWS integration date.</title>
        <desc>Date this bundle, component, or standalone product was integrated into the dWS catalog.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attributeGroup ref="dmy"/>
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 4/19 tdc: renamed date to date-published -->
  <!-- ================================================================ -->
  <!-- <date-published> element                                                   -->
  <!-- ================================================================ -->
  <xsd:element name="date-published">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>The date this article was published.</title>
        <desc>Date published on the supersite or local site.</desc>
        <note>Automatically update the DC.Date and IBM.Effective meta tags with this date - phase 2 of schema/stylesheet.  Date must be conform to ibm.com meta tag standards.</note>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attributeGroup ref="dmy"/>
    </xsd:complexType>
    <!-- ** Should be an XML Schema date type. -->
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <date-updated> element                                           -->
  <!-- ================================================================ -->
  <xsd:element name="date-updated">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Date the document's *content* is updated.</title>
        <desc>Date the document's *content* is updated.  Not a system date, and does not include updates for typos, etc.  Stylesheet automatically updates the d.c. date meta tag with this date, if present.  Date must be presented in international format.  Internally can have unbounded number of updates but UI should only show last update.</desc>
        <note>Automatically update the DC.Date and IBM.Effective meta tags with this date - phase 2 of schema/stylesheet.  Date must be conform to ibm.com meta tag standards.    Internally can have unbounded number of updates but UI should only show last update.</note>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attributeGroup ref="dmy"/>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <dd> (definition) element                                        -->
  <!-- ================================================================ -->
  <xsd:element name="dd">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>The definition of a term</title>
        <desc>This element contains the definition of a term in a definition list.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="dl"/>
        <xsd:element ref="i"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="p"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:element ref="ul"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <dl> (definition list) element                                   -->
  <!-- ================================================================ -->
  <xsd:element name="dl">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>A definition list</title>
        <desc>Defines a definition list.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence maxOccurs="unbounded">
        <xsd:element ref="dt"/>
        <xsd:element ref="dd"/>
      </xsd:sequence>
      <xsd:attribute name="compact" type="yesno">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether or not this definition list should be compacted.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <docbody> element                                                -->
  <!-- ================================================================ -->
  <!-- ** Do we want this to contain text?  I don't think so... -->
  <xsd:element name="docbody">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define the content of a document</title>
        <desc>Defines the content of a document.  This element can contain text, along with the various elements that define the basic layout of text elements.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="blockquote"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="dl"/>
        <xsd:element ref="figure"/>
        <xsd:element ref="heading"/>
        <xsd:element ref="i"/>
        <xsd:element ref="img"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="p"/>
        <xsd:element ref="pre"/>
        <xsd:element ref="sidebar"/>
        <xsd:element ref="table"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:element ref="ul"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 5/10 tdc:  use target-content instead of download-feature -->
  <!-- ================================================================ -->
  <!-- <dt> element                                                     -->
  <!-- ================================================================ -->
  <xsd:element name="dt">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define a term</title>
        <desc>Defines a term in a definition list.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  
  <!-- 4.0 6/7 tdc:  Added dws-additional-text element -->
  <!-- ================================================================ -->
  <!-- <dws-additional-text> element                                       -->
  <!-- ================================================================ -->
  <xsd:element name="dws-additional-text">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title></title>
        <desc>Optional text that appears at the beginning of a given page section.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="p" minOccurs="0" maxOccurs="1"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  
  
  
  <!-- 4.0 5/04 tdc:  renamed from bundleavailability to dws-availability -->
  <!-- ================================================================ -->
  <!-- <dws-availability> element                                       -->
  <!-- ================================================================ -->
  <xsd:element name="dws-availability">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Types of media on which this bundle, component, or product is available. </title>
        <desc>Types of media on which this bundle, component, or product is available.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence minOccurs="0">
        <xsd:element ref="dws-issue-cd"/>
        <!-- 4.0 5/04 tdc:  made "cd" an element which also includes range of cd's the bundle is available on. -->
      </xsd:sequence>
      <xsd:attribute name="custom-cd" type="yesno">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether or not this  bundle is available on a custom CD-ROM.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="web" type="yesno">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether or not this bundle is available on the Web.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <!-- 4.0 5/04 tdc:  removed cdonly per kb -->
      <!-- 4.0 5/04 tdc:  renamed customcd to custom-cd -->
      <!-- 4.0 5/05 tdc:  removed web-only per kb -->
    </xsd:complexType>
  </xsd:element>
  
  <!-- ================================================================ -->
  <!-- <dws-bundle-title> element                                                  -->
  <!-- ================================================================ -->
  <xsd:element name="dws-bundle-title">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title></title>
        <desc>Title of the parent bundle.  dws-subscription-landing pages use the title element (so that the title appears in search).  Component pages use the title element for their own title, and dws-bundle-title to refer back to the parent bundle page.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="code"/>
        <xsd:element ref="i"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  
  <!-- 4.0 6/7 tdc:  Added dws-download element -->
  <!-- ================================================================ -->
  <!-- <dws-download> element                                       -->
  <!-- ================================================================ -->
  <xsd:element name="dws-download">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Section containing links to multiple developerWorks Subscription component files.</title>
        <desc>Section containing links to multiple developerWorks Subscription component files.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="heading"/>
        <xsd:element ref="dws-additional-text" minOccurs="0" maxOccurs="1"/>
        <!-- 4.0 6/16 tdc:  Use dws-additional-license-text attribute here instead of the element 
        <xsd:element ref="dws-additional-license-text" minOccurs="0" maxOccurs="1"/> -->
        <xsd:element ref="target-content-file" maxOccurs="unbounded"/>
      </xsd:sequence>
      <xsd:attribute name="dws-additional-license-text" type="yesno">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Defines whether or not additional licensing text should appear after the standard licensing text.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  
  <!-- 4.0 6/3 tdc:  Added install instructions -->
  <!-- ================================================================ -->
  <!-- <dws-install-instructions>  element                                   -->
  <!-- ================================================================ -->
  <xsd:element name="dws-install-instructions">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title></title>
        <desc>Installation instructions for dW Subscription products.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
        <xsd:sequence minOccurs="1" maxOccurs="1">
           <xsd:element ref="heading"/>           
           <!-- 4.0 8/06 tdc:  Sera discovered the need to allow repeats of this group multiple times -->  
           <xsd:element ref="dws-instruction-groups" minOccurs="1" maxOccurs="1"/>           
        </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <!-- 4.0 4/15 tdc: created dws-issue-cd so that dws availability section can indicate range of cd numbers. -->
  <!-- ================================================================ -->
  <!-- <dws-issue-cd> element                                                   -->
  <!-- ================================================================ -->
  <xsd:element name="dws-issue-cd">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>If available on CD, indicate CD number(s)</title>
        <desc>Indicates whether the bundle, component, or product is available on CD, and if so, the CD number(s) it appears on.  If only one CD number, use range-bottom only.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:attribute name="include" type="yesno" use="required"/>
      <!-- 4.0 6/11 tdc:  Changed datatypes of both range values from positiveInteger to string -->
      <xsd:attribute name="range-bottom" type="xsd:string" use="required"/>
      <xsd:attribute name="range-top" type="xsd:string" use="optional"/>
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 5/04 tdc:  renamed from bundlelanguages to dws-languages -->
  <!-- ================================================================ -->
  <!-- <dws-languages> element                                                -->
  <!-- ================================================================ -->
  <!-- ** Do we want this to contain text?  I don't think so... -->
  <xsd:element name="dws-languages">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Defines the content of the Languages section for developerWorks Subscription pages.</title>
        <desc>Defines the content of the Languages section for developerWorks Subscription pages.  This element can contain text, along with the various elements that define the basic layout of text elements.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="blockquote"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="dl"/>
        <xsd:element ref="figure"/>
        <xsd:element ref="heading"/>
        <xsd:element ref="i"/>
        <xsd:element ref="img"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="p"/>
        <xsd:element ref="pre"/>
        <xsd:element ref="sidebar"/>
        <xsd:element ref="table"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:element ref="ul"/>
        <xsd:group ref="specialCharacters"/>
        <!-- I removed <dt> and <dd> -->
        <!-- I removed <li> -->
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  
  <!-- 4.0 6/3 tdc:  Added license text -->
  <!-- 4.0 6/7 tdc:  Changed name to dws-additional-license-text -->
  <!-- 4.0 6/16 tdc:  Replaced this element with a yes/no attrib (xsl contains text) -->
  <!-- ================================================================ -->
  <!-- <dws-additional-license-text>  element                                   -->
  <!-- ================================================================ 
  <xsd:element name="dws-additional-license-text">
    <xsd:annotation>
