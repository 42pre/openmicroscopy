  <xsd:element name="trademark">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Trademark attribution</title>
        <desc>Trademark statements are used to give proper (legal) attribution to content displayed on dW with permission from the trademark owner.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="i"/>
        <xsd:element ref="ul"/>
        <xsd:element ref="p"/>
        <xsd:group ref="specialCharacters"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  <!-- 4.0 4/29 tdc: Added to group all trademarks together. -->
  <!-- ================================================================ -->
  <!-- <trademarks> element                                               -->
  <!-- ================================================================ -->
  <xsd:element name="trademarks">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Trademarks</title>
        <desc>Contains one or more trademark elements.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element ref="heading" minOccurs="0"/>
        <xsd:element ref="trademark" maxOccurs="unbounded"/>
        <!-- Trademarks may appear under a heading or not -->
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <!-- ================================================================ -->
  <!-- <ul> (unordered list) element                                    -->
