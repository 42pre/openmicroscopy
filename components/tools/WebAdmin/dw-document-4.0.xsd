  <xsd:element name="ul">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <title>Define an unordered list</title>
        <desc>Defines an unordered list.  The list contains one or more list items.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:choice maxOccurs="unbounded">
        <xsd:element ref="li"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="ul"/>
      </xsd:choice>
