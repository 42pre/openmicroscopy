      <xsd:documentation xml:lang="en">
        <desc>The classification of a particular heading.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="code">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A heading (caption) for a code listing</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="figure">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A heading (caption) for a figure.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="major">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A major heading.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="minor">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A minor heading.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="resource-list">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The heading for the list of related resources.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="sidebar">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>A heading for a sidebar.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ================================================================ -->
  <!-- inline - should an item be inline or a separate section?         -->
  <!-- ================================================================ -->
  <xsd:simpleType name="inline">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="inline">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>This item should be formatted inline.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="section">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>This item should be formatted as a separate section.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- 4.0 4/15 tdc: renamed level to skill-level -->
  <!-- ================================================================ -->
  <!-- skill-level - Indicates complexity and depth of content contained in the document, from general background information for novices to advanced technical details for more experienced audience members         -->
  <!-- ================================================================ -->
  <xsd:simpleType name="skill-level">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <desc>Indicates complexity and depth of content contained in the document, from general background information for novices to advanced technical details for more experienced audience members.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:positiveInteger">
      <xsd:enumeration value="1">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Limited knowledge, no experience</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="2">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>General knowledge, limited experience</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="3">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Applied knowledge, can perform routine tasks without assistance</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="4">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>In-depth knowledge, can perform all tasks without assitance</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="5">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Extensive knowledge and experience</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- 4.0 6/10 tdc:  Changed to enumeration; single digits get a zero prefix -->
  <!-- ================================================================ -->
  <!-- month - the month of the year -->
  <!-- ================================================================ --> 
  <xsd:simpleType name="month">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="01"/>
      <xsd:enumeration value="02"/>
      <xsd:enumeration value="03"/>
      <xsd:enumeration value="04"/>
      <xsd:enumeration value="05"/>
      <xsd:enumeration value="06"/>
      <xsd:enumeration value="07"/>
      <xsd:enumeration value="08"/>
      <xsd:enumeration value="09"/>
      <xsd:enumeration value="10"/>
      <xsd:enumeration value="11"/>
      <xsd:enumeration value="12"/>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- 4.0 4/26 tdc:  No longer need origin -->
  <!-- ================================================================ -->
  <!-- paperSize - The size of paper used in a PDF file                 -->
  <!-- ================================================================ -->
  <xsd:simpleType name="paperSize">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="a4">
        <xsd:annotation>
          <xsd:documentation>
            <desc>A4-sized paper, used in most of the world.  Sheets are 210 by 297 millimeters, roughly 8.26 by 11.69 inches.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="letter">
        <xsd:annotation>
          <xsd:documentation>
            <desc>Letter-sized paper, used primarily in the United States.  Sheets are 8.5 by 11 inches, roughly 216 by 279 millimeters.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- 4.0 04/21/04 tdc:  removed recentyear and updatedyear -->
  <!-- ================================================================ -->
  <!-- site - where is this document from?                              -->
  <!-- ================================================================ -->
  <xsd:simpleType name="site">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <desc>Indicates whether the origin of this content is the worldwide site or one of the local sites</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="worldwide">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The worldwide developerWorks site.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="china">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The dW China site.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="german">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The dW Germany site.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="india">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The dW India site.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="japan">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The dW Japan site.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="korea">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The dW Korea site.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="taiwan">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The dW Taiwan site.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="uk">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The dW United Kingdom site.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- 4.0 4/15 tdc: changed subscriptionlevel to subscription-level -->
  <!-- ================================================================ -->
  <!-- subscription-level - the possible developerWorks Subscription levels that users may have.         -->
  <!-- ================================================================ -->
  <!-- 4.0 6/18 tdc:  Changed restriction base from positive integer to string -->
  <xsd:simpleType name="subscription-level">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <desc>The possible developerWorks Subscription levels that users may have.  </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="1">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Starter</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="2">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Professional</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="3">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Enterprise</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  
  <!-- 4.0 6/1 tdc:  Added as simpleType to enable limit of 50 char's -->
  <!-- ================================================================ -->
  <!-- file-description-50 - A short description (50 characters or less) of a target content file.     -->
  <!-- ================================================================ -->
  <xsd:simpleType name="file-description-50">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <desc>A short description (50 characters or less) of a target content file.  No formatting characters are allowed.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="50"/>
    </xsd:restriction>
  </xsd:simpleType>  
  
  <!-- ================================================================ -->
  <!-- vertical - vertical alignment values                             -->
  <!-- ================================================================ -->
  <xsd:simpleType name="vertical">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <desc>The vertical alignment of the content.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="bottom">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Vertically aligns the content along the bottom.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="middle">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Vertically aligns the content in the middle.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="top">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Vertically aligns the content at the top.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ================================================================ -->
  <!-- xslfoColor - one of the 16 color names from XSL-FO.              -->
  <!-- ================================================================ -->
  <xsd:simpleType name="xslfoColor">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <desc>One of the sixteen color names defined in the XSL-FO standard, or a hexadecimal color code, as in <code>#FF00FF</code> or <code>FF00FF</code>.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="aqua"/>
      <xsd:pattern value="black"/>
      <xsd:pattern value="blue"/>
      <xsd:pattern value="gray"/>
      <xsd:pattern value="fuchsia"/>
      <xsd:pattern value="gray"/>
      <xsd:pattern value="green"/>
      <xsd:pattern value="lime"/>
      <xsd:pattern value="maroon"/>
      <xsd:pattern value="navy"/>
      <xsd:pattern value="olive"/>
      <xsd:pattern value="purple"/>
      <xsd:pattern value="red"/>
      <xsd:pattern value="silver"/>
      <xsd:pattern value="teal"/>
      <xsd:pattern value="white"/>
      <xsd:pattern value="yellow"/>
      <xsd:pattern value="[#]?[A-Fa-f0-9]{6}"/>
    </xsd:restriction>
    <!-- 4/7/03 Doug changed the colors to allow names and hex -->
  </xsd:simpleType>
  <!-- ================================================================ -->
  <!-- yesno - a simple yes or no.                                      -->
  <!-- ================================================================ -->
  <xsd:simpleType name="yesno">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="yes"/>
      <xsd:enumeration value="no"/>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ================================================================ -->
  <!-- year - the value for year must be reasonable               -->
  <!-- ================================================================ -->
  <xsd:simpleType name="year">
    <xsd:restriction base="xsd:positiveInteger">
      <xsd:minInclusive value="1996"/>
      <xsd:maxInclusive value="2010"/>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ================================================================ -->
  <!-- zeroOrOne - either 0 or 1.                                       -->
  <!-- ================================================================ -->
  <xsd:simpleType name="zeroOrOne">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <desc>Whether this item has a border.  A value of <code>0</code> means no border, <code>1</code> means there will be one.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:integer">
      <xsd:minInclusive value="0"/>
      <xsd:maxInclusive value="1"/>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- ================================================================ -->
  <!-- content-area - the dW content area for this document.                            -->
  <!-- ================================================================ -->
  <xsd:simpleType name="content-area">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <desc>The developerWorks content-area to which this article belongs.</desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="autonomic">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The Autonomic computing content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="components">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The components content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="db2">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The DB2 product area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="eserver">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The eServer product area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="grid">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The Grid computing content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="ibm">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The IBM content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="java">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The Java content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="linux">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The Linux content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="lotus">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The Lotus product area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="none">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>Doesn't belong to any content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="opensource">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The open source content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <!-- 4.0 9/15 tdc:  Added Power -->
      <xsd:enumeration value="power">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The Power Architecture content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="rational">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The Rational product area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="security">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The security content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="tivoli">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The Tivoli product area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="subscription">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The developerWorks Subscription area (formerly "Toolbox").</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="unicode">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The Unicode content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="usability">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The usability content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="web">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The Web architecture content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="webservices">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The Web services content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="websphere">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The WebSphere product area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="wireless">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The wireless content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="xml">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The XML content-area.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>
