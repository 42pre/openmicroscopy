  <!-- ================================================================ -->
  <!-- <font> element                                                   -->
  <!-- ================================================================ -->
  <xsd:element name="font">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <desc>Defines the font to be used for this content. </desc>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType mixed="true">
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="a"/>
        <xsd:element ref="b"/>
        <xsd:element ref="br"/>
        <xsd:element ref="code"/>
        <xsd:element ref="dl"/>
        <xsd:element ref="font"/>
        <xsd:element ref="i"/>
        <xsd:element ref="ol"/>
        <xsd:element ref="sub"/>
        <xsd:element ref="sup"/>
        <xsd:element ref="table"/>
        <xsd:element ref="ul"/>
        <xsd:group ref="specialCharacters"/>
        <!-- <font> is evil, must be killed -->
      </xsd:choice>
      <xsd:attribute name="color" type="xslfoColor">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The color for the font.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="face" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The face name used for this font.</desc>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
      <xsd:attribute name="size">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <desc>The size of the font. </desc>
