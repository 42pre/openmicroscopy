<ivy-module version="1.0">
  <info
      organisation="omero"
      module="distribution"
      status="integration"/>
  <configurations>
    <conf name="client"/>
    <conf name="server"/>
  </configurations>
  <publications>
    <artifact name="server" type="jar" conf="server" />
  </publications>
  <dependencies>
    <!-- NOTE: Jars from lib/tools will be copied separately -->
    <!-- Internal -->
    <dependency name="client" rev="${omero.version}" changing="true" conf="client->client"/>
    <dependency name="server" rev="${omero.version}" changing="true" conf="server->server"/>
    <dependency name="blitz" rev="${omero.version}" changing="true" conf="server->server;client->client"/>
    <!-- For java omero.class -->
    <dependency org="ant" name="ant-launcher" rev="${versions.ant}" conf="server->default"/>
    <dependency org="ant" name="ant-nodeps" rev="${versions.ant}" conf="server->default"/>
    <dependency org="ant" name="ant" rev="${versions.ant}" conf="server->default"/>
 </dependencies>
</ivy-module>
